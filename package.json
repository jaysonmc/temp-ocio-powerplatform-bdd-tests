{
  "name": "ocio-powerplatform-bdd",
  "version": "0.0.1",
  "engines": {
    "node": ">=15"
  },
  "description": "bdd tests for ocio-powerplatform",
  "scripts": {
    "test": "HAIBUN_TRACE=true HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true haibun-cli test",
    "clear-test": "HAIBUN_SETTING='test' haibun-cli clear",
    "publish": "haibun-cli publish",
    "publish-local": "haibun-cli publish-local",
    "index-sarif": "haibun-cli index-sarif/ ",
    "index-sarif-local": "haibun-cli index-sarif-local/ ",
    "test-regressions": "haibun-cli test regressions/",

    "test-pipeline-login": "HAIBUN_TRACE=true HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true haibun-cli test account-login/",
    "test-pipeline-confirmatory-inconclusive": "HAIBUN_TRACE=true HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true haibun-cli test y-inconclusive",
    "test-pipeline-positive": "HAIBUN_TRACE=true HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true haibun-cli test positive",
    "test-pipeline-regressions": "HAIBUN_TRACE=true HAIBUN_O_WEBPLAYWRIGHT_CAPTURE_VIDEO=true haibun-cli test regressions/",
    "test-pipeline-index-sarif": "haibun-cli index-sarif",
    "test-pipeline-indexes": "haibun-cli publish",

    "test-lang": "haibun-cli test lang/",
    "test-100x1": "HAIBUN_LOOPS=100 HAIBUN_MEMBERS=1 haibun-cli test",
    "test-2x2": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=2 haibun-cli test",
    "test-2x8": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=8 haibun-cli test",
    "test-2x32": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=32 haibun-cli test",
    "test-313x32": "HAIBUN_LOOPS=312 HAIBUN_MEMBERS=32 haibun-cli test",
    "test-2x64": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=64 haibun-cli test",
    "test-2x128": "HAIBUN_LOOPS=2 HAIBUN_MEMBERS=128 haibun-cli test",
    "test-1x500": "HAIBUN_LOOPS=1 HAIBUN_MEMBERS=500 haibun-cli test",
    "test-40x250": "HAIBUN_LOOPS=40 HAIBUN_MEMBERS=250 haibun-cli test",
    "test-10000": "HAIBUN_LOOPS=400 HAIBUN_MEMBERS=25 haibun-cli test",
    "test-20000": "HAIBUN_LOOPS=400 HAIBUN_MEMBERS=50 haibun-cli test"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@haibun/azure": "^1.9.0",
    "@haibun/cli": "^1.9.0",
    "@haibun/core": "^1.9.0",
    "@haibun/domain-storage": "^1.9.0",
    "@haibun/domain-webpage": "^1.9.0",
    "@haibun/out-review": "^1.9.0",
    "@haibun/out-xunit": "^1.9.0",
    "@haibun/sarif": "^1.9.0",
    "@haibun/storage-fs": "^1.9.0",
    "@haibun/web-playwright": "^1.9.0",
    "@haibun/web-server-express": "^1.9.0"
  }
}
